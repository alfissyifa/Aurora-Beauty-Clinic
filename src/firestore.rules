
rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
      // WARNING: These rules are wide open for development purposes.
      // Anyone can read, write, and delete any data in your database.
      // Make sure to secure your rules before going to production.
      allow read, write: if true;
    }
  }
}
